extends layout

block content
  h3 账户
  table.table
    thead
      tr
        th 地址
        th 类型
        th 余额
    tbody
      for account in accounts
        tr
          td 
            a(href="/account/" + account.address) #{nameformatter.format(account.address)}
          td #{account.type}
          td #{ethformatter(account.balance)}
  a.btn.btn-default(href="/accounts/" + lastAccount) 下一页